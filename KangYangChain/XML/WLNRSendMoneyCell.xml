<UIView layout="flex:1" attr="@:css/colors">
    
    <UIView layout="margin:10" attr="bgColor:white,borderRadius:10">
    
    <FlexTouchView onPress="chooseAction" layout="margin:10,flexDirection:row,justifyContent:space-between" attr="underlayColor:darkGray,borderRadius:7,
        ">
        <UILabel name="" layout="" attr="fontSize:14,color:#333333,linesNum:0,text:币种选择"/>
        <UILabel name="typeLab" layout="margin:15" attr="fontSize:14,color:#333333,linesNum:0,text:请选择"/>
        <UIImageView attr="source:goback" layout="margin:15"/>
    </FlexTouchView>


        
    <UIView layout="flexDirection:row,margin:10,marginTop:0" attr="">
        

        <UILabel name="" layout="" attr="fontSize:14,color:#333333,linesNum:0,text:提现地址"/>
        <UITextField name="addressTxt" layout="height:40,marginLeft:10,flex:1" attr="fontSize:14,color:black,textAlign:left,placeholder:,text:,placeholder:输入或长按粘贴地址"/>
        
        <FlexTouchView onPress="saoAction" layout="width:40,height:40,marginRight:0,justifyContent:center,alignItems:center" attr="underlayColor:darkGray">
            <UIImageView attr="source:Topromote" layout="width:25,height:20"/>
        </FlexTouchView>
        
    </UIView>
    
    <UIView layout="flexDirection:row,margin:10" attr="">
        

        <UILabel name="" layout="" attr="fontSize:14,color:#333333,linesNum:0,text:提币数量"/>
        <UITextField name="numTxt" layout="height:40,marginLeft:10,flex:1" attr="fontSize:14,color:black,textAlign:left,placeholder:,text:,placeholder:输入数量,keyboardType:numberPad"/>
     
    </UIView>

    <UIView layout="flex:1,flexDirection:row,justifyContent:space-between" attr="@:css/colors">
        <UILabel name="" layout="margin:5" attr="fontSize:14,color:#333333,linesNum:0,text:可提币数量0.00BTC"/>
        <FlexTouchView onPress="" layout="justifyContent:center,alignItems:center" attr="underlayColor:darkGray">
            <UILabel name="" layout="" attr="fontSize:14,color:#333333,linesNum:0,text:全部提现"/>
        </FlexTouchView>

    </UIView>

    <UIView layout="flexDirection:row" attr="">
        <UILabel name="" layout="margin:10" attr="fontSize:14,color:#333333,linesNum:0,text:网络手续费"/>
        <UILabel name="" layout="" attr="fontSize:14,color:#333333,linesNum:0,text:0.05"/>
    </UIView>

    <UIView layout="flexDirection:row" attr="">
        <UILabel name="" layout="margin:10" attr="fontSize:14,color:#333333,linesNum:0,text:短信验证码"/>
        <UITextField name="" layout="flex:1" attr="fontSize:14,color:black,textAlign:left,placeholder:,text:"/>
        <FlexTouchView onPress="" layout="marginRight:10" attr="underlayColor:darkGray">
            <UILabel name="" layout="" attr="fontSize:14,color:#333333,linesNum:0,text:获取验证码"/>
        </FlexTouchView>
    </UIView>

    <UIView layout="flexDirection:row" attr="">
        <UILabel name="" layout="margin:10" attr="fontSize:14,color:#333333,linesNum:0,text:邮箱验证码"/>
        <UITextField name="" layout="flex:1" attr="fontSize:14,color:black,textAlign:left,placeholder:,text:"/>
        <FlexTouchView onPress="" layout="marginRight:10" attr="underlayColor:darkGray">
            <UILabel name="" layout="" attr="fontSize:14,color:#333333,linesNum:0,text:获取验证码"/>
        </FlexTouchView>
    </UIView>

    </UIView>


    <FlexTouchView onPress="showPwd" layout="margin:10,height:40,justifyContent:center,alignItems:center" attr="underlayColor:darkGray,bgColor:#41559D,borderRadius:5">
        <UILabel name="" layout="" attr="fontSize:14,color:white,linesNum:0,text:发送"/>
    </FlexTouchView>
        
    


</UIView>

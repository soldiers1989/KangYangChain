            </UIView>

            <UIView layout="margin:10,marginTop:0" attr="">
                <UILabel name="" layout="" attr="fontSize:12,color:#333333,linesNum:0,text:成交量"/>
                <UILabel name="" layout="" attr="fontSize:12,color:#333333,linesNum:0,text:0.12"/>
            </UIView>

            <UIView layout="margin:10,marginTop:0" attr="">

                <UILabel name="" layout="" attr="fontSize:12,color:#333333,linesNum:0,text:委托价格"/>
                <UILabel name="" layout="" attr="fontSize:12,color:#333333,linesNum:0,text:$5444"/>
            
            </UIView>

        </UIView>
    </UIView>

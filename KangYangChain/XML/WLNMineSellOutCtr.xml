<UIView layout="marginTop:30" attr="bgColor:#f5f5f5">

    <UIView layout="flexDirection:row" attr="bgColor:white">
        <UILabel name="" layout="margin:15" attr="fontSize:14,color:#333333,linesNum:0,text:数量(GHB)"/>
        <UITextField name="countTxt" layout="flex:1" attr="fontSize:14,color:gray,textAlign:left,placeholder:请输入交易数量,text:,keyboardType:numberPad"/>
    </UIView>

    <UILabel name="" layout="margin:10" attr="fontSize:14,color:#333333,linesNum:0,text:最高可出售:0.0GHB"/>

    <UIView layout="flexDirection:row" attr="bgColor:white">
        <UILabel name="" layout="" attr="fontSize:14,color:#333333,linesNum:0,text:交易说明"/>
        <FlexTextView name="" layout="flex:1,height:100" attr="fontSize:14,color:black,textAlign:left,placeholder:xxxxxxx"/>
    </UIView>

    <FlexTouchView onPress="sellAction" layout="margin:20,justifyContent:center,alignItems:center,height:40" attr="underlayColor:darkGray,bgColor:#1E253D,
    borderRadius:7">
        <UILabel name="" layout="" attr="fontSize:14,color:white,linesNum:0,text:发布"/>
    </FlexTouchView>



</UIView>


